{
  "manifest_version": 2,

  "name": "Workaholic's Bane",
  "description": "This extension stops users from using the Internet at specified times",
  "version": "1.0",

  "background": {
	"scripts": ["eventPage.js"],
	"persistent": false
  },
  "browser_action": {
    "default_icon": "balance.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
	"alarms",
	"storage",
	"tabs"
  ]
}